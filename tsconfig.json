{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "lib": [
      "es2017"
    ],
    "strict": true,
    "target": "es2017",
    "types": [
      "chai",
      "mocha",
      "node"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
